<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="task-flow-definition">
    <default-activity>dept1</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="dept1">
      <page>/first_pages_deri27/dept1.jsff</page>
    </view>
    <view id="emp1">
      <page>/first_pages_deri27/emp1.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>dept1</from-outcome>
        <to-activity-id>dept1</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__3">
        <from-outcome>emp1</from-outcome>
        <to-activity-id>emp1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
